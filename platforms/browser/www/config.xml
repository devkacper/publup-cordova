<?xml version='1.0' encoding='utf-8'?>
<widget id="com.publup.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Publup</name>
    <description>
        Easier, faster and more effective way to express yourself.
    </description>
    <author email="theofficialpublup@gmail.com" href="https://publup.com/pages/contact">
        Publup contact
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="res/ios/screen/Default@2x~universal~anyany.png" />
    <preference name="BackgroundColor" value="0x202124" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#202124" />
</widget>
